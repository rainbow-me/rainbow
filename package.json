{
  "name": "Rainbow",
  "version": "1.2.34-1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:bundle": "cd android && ./gradlew bundleRelease && open app/build/outputs/bundle/release/",
    "android:apk": "cd android && ./gradlew assembleRelease && adb install app/build/outputs/apk/release/app-release.apk",
    "clean": "react-native clean-project-auto",
    "detox:ios": "detox build -c ios.sim.debug && detox test -c ios.sim.debug",
    "detox:ios:release": "detox build -c ios.sim.release && detox test -c ios.sim.release",
    "detox:android": "detox build -c android.emu.debug && detox test -c android.emu.debug --loglevel verbose",
    "detox:android:release": "detox build -c android.emu.release && detox test -c android.emu.release",
    "start": "react-native start",
    "start:clean": "watchman watch-del-all && rm -rf $TMPDIR/react-* && react-native start -- --reset-cache",
    "test": "jest test.js",
    "install-all": "yarn && yarn install-pods",
    "install-all-no-flipper": "yarn && yarn install-pods-no-flipper",
    "install-pods": "cd ios && bundle exec pod install --repo-update && cd ..",
    "install-pods-no-flipper": "cd ios && bundle exec env SKIP_FLIPPER=true pod install --repo-update && cd ..",
    "install-bundle": "cd ios && bundle install && cd ..",
    "ios": "react-native run-ios --simulator='iPhone 11'",
    "lint": "yarn lint:ts && yarn lint:js && yarn lint:css",
    "lint:css": "stylelint './src/**/*.js'",
    "lint:js": "eslint --ext '.ts,.js,.jsx' .",
    "lint:ts": "yarn tsc --skipLibCheck --noEmit",
    "postinstall": "./scripts/postinstall.sh",
    "update-env": "curl -H 'Authorization: token '$GITHUB_TOKEN -H 'Accept: application/vnd.github.v3.raw' -O  -L https://api.github.com/repos/rainbow-me/rainbow-env/contents/dotenv && rm -f .env && mv dotenv .env",
    "ganache": "source .env && ganache-cli --f https://mainnet.infura.io/v3/${INFURA_PROJECT_ID_DEV} -p 7545 -i 1 -b 10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts}": [
      "eslint --ext '.js,.jsx,.ts' -c .eslintrc.json",
      "stylelint"
    ]
  },
