{
  "name": "Rainbow",
  "version": "1.2.6-0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:bundle": "cd android && ./gradlew bundleRelease && open app/build/outputs/bundle/release/",
    "android:apk": "cd android && ./gradlew assembleRelease && adb install app/build/outputs/apk/release/app-release.apk",
    "clean": "react-native clean-project-auto",
    "detox:ios": "detox build -c ios.sim.debug && detox test -c ios.sim.debug",
    "detox:ios:release": "detox build -c ios.sim.release && detox test -c ios.sim.release",
    "detox:android": "detox build -c android.emu.debug && detox test -c android.emu.debug --loglevel verbose",
    "detox:android:release": "detox build -c android.emu.release && detox test -c android.emu.release",
    "start": "react-native start",
    "start:clean": "watchman watch-del-all && rm -rf $TMPDIR/react-* && react-native start -- --reset-cache",
    "test": "jest test.js",
    "install-pods": "cd ios && pod install && cd ..",
    "ios": "react-native run-ios --simulator='iPhone X'",
    "lint": "eslint --ext '.js,.jsx' .",
    "nodeify": "./node_modules/.bin/rn-nodeify --install \"crypto\" --hack",
    "postinstall": "patch-package && rn-nodeify --install --hack"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "eslint --ext '.js,.jsx,.json' -c .eslintrc.json",
      "git add"
    ]
  },
